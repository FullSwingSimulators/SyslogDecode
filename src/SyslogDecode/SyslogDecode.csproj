<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon />
    <OutputType>Library</OutputType>
    <StartupObject />
    <Product>Syslog</Product>
    <Description>Syslog client (sender), server (listener) components.</Description>
    <Copyright>Full Swing</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageVersion>1.3.0</PackageVersion>
    <Version>1.3.0</Version>
    <Authors>Full Swing</Authors>
    <Company>Full Swing</Company>
    <RepositoryUrl>https://github.com/FullSwingSimulators/SyslogDecode</RepositoryUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
    <GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
    <GenerateAssemblyInformationalVersionAttribute>false</GenerateAssemblyInformationalVersionAttribute>
    <AssemblyName>SyslogDecode</AssemblyName>
    <RootNamespace>SyslogDecode</RootNamespace>
  </PropertyGroup>

</Project>
